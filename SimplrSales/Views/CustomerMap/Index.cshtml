                tr.append("<td style='border:1px solid navy;'>" + Input_Details[i].Latitude + "</td>");
                tr.append("<td style='border:1px solid navy;'>" + Input_Details[i].Longitude + "</td>");
                $('#InTable').append(tr);
            }
        }

        function Display_Output_Data() {

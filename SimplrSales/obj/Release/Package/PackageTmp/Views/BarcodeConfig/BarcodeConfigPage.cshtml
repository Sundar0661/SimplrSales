        function SafeSQL(datavalue) {
            return "'" + datavalue + "'"
        }

        function saveMETHOD() {
           // debugger;
            var item_Length = main_Object.item_Start == -1 ? 0 : parseInt(main_Object.item_End) - parseInt(main_Object.item_Start) + 1;
            var lot_Length = main_Object.lot_Start == -1 ? 0 : parseInt(main_Object.lot_End) - parseInt(main_Object.lot_Start) + 1;
            var date_Length = main_Object.date_Start == -1 ? 0 : parseInt(main_Object.date_End) - parseInt(main_Object.date_Start) + 1;
            var weight_Length = main_Object.weight_Start == -1 ? 0 : parseInt(main_Object.weight_End) - parseInt(main_Object.weight_Start) + 1;

            var query = '';

            query = "UPDATE BarcodeConfig SET ";
            query += " SupplierBarcode = " + SafeSQL(given_Barcode.substring(parseInt(main_Object.item_Start), parseInt(main_Object.item_End) + 1)) + ",";
            query += " ItemID_Start = " + SafeSQL(main_Object.item_Start) + ",";
            query += " ItemID_Length = " + SafeSQL(item_Length) + ",";
            query += " LotNo_Start = " + SafeSQL(main_Object.lot_Start) + ",";
            query += " LotNo_Length = " + SafeSQL(lot_Length) + ",";
            query += " ExpiryDate_Start = " + SafeSQL(main_Object.date_Start) + ",";
            query += " ExpiryDate_Length = " + SafeSQL(date_Length) + ",";
            query += " Weight_Start = " + SafeSQL(main_Object.weight_Start) + ",";
            query += " Weight_Length = " + SafeSQL(weight_Length) + ",";
            query += " ExpiryDate_Format = " + SafeSQL(main_Object.date_Format) + ",";
            query += " Weight_Format = " + SafeSQL(main_Object.weight_Format) + ",";
            query += " ExpiryDate_CalcPeriod = " + SafeSQL(main_Object.date_CalcPeriod) + ",";
            query += " ExpiryDate_Calc = " + SafeSQL(main_Object.date_CalcNo) + "";
            query += " WHERE  barcode =" + SafeSQL(given_Barcode);

            //alert(query);

            $.ajax({
                url: url_Barcode_ExecuteNonQuery,
                type: 'POST',
                dataType: 'json',
                async: false,
                data: { sqlqry: query },
                success: function (results) {
                    if (results == "1") {

                        var obj = {};
                        obj.title = "Information";
                        obj.message = 'Saved Successfully.';
                        _showAlertMessage(obj.title, obj.message);

                        //alert('Saved Successfully.');
                        //window.location = url_FormClickEvent + "?ScreenName=BarcodeConfigList" + "&FieldName=''";
                        //var obj = {};
                        //obj.title = "Information";
                        //obj.message = 'Saved Successfully.';
                        //showAlertMessage(obj);

                        //setTimeout(function () { window.location = url_FormClickEvent + "?ScreenName=BarcodeConfigList" + "&FieldName=''"; }, 300);
                    }
                    else {
                        //alert('Failed. please check.');
                        var obj = {};
                        obj.title = "Information";
                        obj.message = 'Failed. please check.';
                        showAlertMessage(obj);
                    }
                },
                error: function (xhr) {
                    alert('Request Status: ' + xhr.status + ' Status Text: ' + xhr.statusText + ' ' + xhr.responseText);
                }
            });

        }

        function _showAlertMessage(title, message) {
            var btns = {};
            btns["Ok"] = function (e) {
                $(this).dialog("close");
                window.location = url_FormClickEvent + "?ScreenName=BarcodeConfigList" + "&FieldName=''";
            }
            $('<div></div>').appendTo('body')
                                    .html(message).dialog({
                                        modal: true, title: title, zIndex: 10000, autoOpen: true,
                                        width: '35%', resizable: false,
                                        buttons: btns
                                    });
        }

        function clear_barcode_entry() {
            for (i = 1; i < 51; i++) {
                ctrlid = "cell_used_" + i;
                $("#" + ctrlid).removeClass("active");
            }
        }

        function prepare_Controls() {
            if (_main_idx == -1 || _main_idx == 4) {
                return;
            }
            else {
                clear_barcode_entry();

                if (_main_idx == 0) {
                    //alert(0);
                    $("#tblItem").show();
                    $("#tblLot").hide();
                    $("#tblDate").hide();
                    $("#tblWeight").hide();
                    $("#btnNext").val('Next');
                    indicate_Barcode();

                }
                else if (_main_idx == 1) {
                    //alert(1);
                    $("#tblItem").hide();
                    $("#tblLot").show();
                    $("#tblDate").hide();
                    $("#tblWeight").hide();
                    $("#btnNext").val('Next');
                    indicate_Barcode();

                }
                else if (_main_idx == 2) {
                    //alert(2);
                    $("#tblItem").hide();
                    $("#tblLot").hide();
                    $("#tblDate").show();
                    $("#tblWeight").hide();
                    $("#btnNext").val('Next');
                    indicate_Barcode();

                }
                else if (_main_idx == 3) {
                    //alert(3);
                    $("#tblItem").hide();
                    $("#tblLot").hide();
                    $("#tblDate").hide();
                    $("#tblWeight").show();
                    $("#btnNext").val('Save');
                    indicate_Barcode();

                }
            }
        }

    </script>



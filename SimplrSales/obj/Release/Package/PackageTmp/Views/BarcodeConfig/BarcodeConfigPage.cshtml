                $("#expiry").prop("checked", true);
                $("#production").prop("checked", false);
                $("#datePeriod").prop("disabled", true);
                $("#dateCalculation").prop("disabled", true);
                $("#datePeriod").val("");
                $("#dateCalculation").val("");
                $("#datePeriod").css("background-color", "gray");
                $("#dateCalculation").css("background-color", "gray");
            }
            else {
                $("#expiry").prop("checked", false);
                $("#production").prop("checked", true);
                $("#datePeriod").prop("disabled", false);
                $("#dateCalculation").prop("disabled", false);
                $("#datePeriod").css("background-color", "white");
                $("#dateCalculation").css("background-color", "white");

                $("#datePeriod").val(main_Object.date_CalcPeriod);
                $("#dateCalculation").val(main_Object.date_CalcNo);

            }


        }

        function Prepare_Data() {
            if (given_Barcode != '' && given_Barcode != null) {
                //debugger;
                $.ajax({
                    url: url_Barcode_GetBarcodeDetails,
                    type: 'GET',
                    dataType: 'json',
                    async: false,
                    data: { Barcode: given_Barcode },
                    success: function (results) {
                        if (results != null && results != "" && results != undefined) {
                            //debugger;
                            main_Object.item_Start = parseInt(results.BarcodeData[0].ItemID_Start);
                            main_Object.item_End = parseInt(main_Object.item_Start) == -1 ? 0 : parseInt(main_Object.item_Start) + parseInt(results.BarcodeData[0].ItemID_Length) - 1;

                            main_Object.lot_Start = parseInt(results.BarcodeData[0].LotNo_Start);
                            main_Object.lot_End = parseInt(main_Object.lot_Start) == -1 ? 0 : parseInt(main_Object.lot_Start) + parseInt(results.BarcodeData[0].LotNo_Length) - 1;

                            main_Object.date_Start = parseInt(results.BarcodeData[0].ExpiryDate_Start);
                            main_Object.date_End = parseInt(main_Object.date_Start) == -1 ? 0 : parseInt(main_Object.date_Start) + parseInt(results.BarcodeData[0].ExpiryDate_Length) - 1;

                            main_Object.weight_Start = parseInt(results.BarcodeData[0].Weight_Start);
                            main_Object.weight_End = parseInt(main_Object.weight_Start) == -1 ? 0 : parseInt(main_Object.weight_Start) + parseInt(results.BarcodeData[0].Weight_Length) - 1;

                            main_Object.date_Format = results.BarcodeData[0].ExpiryDate_Format;
                            main_Object.weight_Format = results.BarcodeData[0].Weight_Format;

                            main_Object.date_CalcPeriod = results.BarcodeData[0].ExpiryDate_CalcPeriod;
                            main_Object.date_CalcNo = parseInt(results.BarcodeData[0].ExpiryDate_Calc);

                            if (main_Object.date_CalcPeriod != "" && main_Object.date_CalcPeriod != null) {
                                main_Object.date_Type = "production";
                            }
                            else {
                                main_Object.date_Type = "expiry";
                            }
                        }
                    },
                    error: function (xhr) {
                        alert('Failed. To get data please check.');
                        //var obj = {};
                        //obj.title = "Information";
                        //obj.message = 'Failed. To get data please check.';
                        //showAlertMessage(obj);
                    }
                });
            }
        }
        function store_controls_to_entities() {
            if (_main_idx == 0) {
                //alert(0);
                main_Object.item_Start = $("#itemStart").val();
                main_Object.item_End = $("#itemEnd").val();
            }
            else if (_main_idx == 1) {
                //alert(1);
                main_Object.lot_Start = $("#lotStart").val();
                main_Object.lot_End = $("#lotEnd").val();
            }
            else if (_main_idx == 2) {
                //alert(2);
                main_Object.date_Start = $("#dateStart").val();
                main_Object.date_End = $("#dateEnd").val();
                main_Object.date_Format = $("#dateFormat").val();

                if ($("input[name='dateType']:checked").val() == "expiry") {
                    main_Object.date_Type = "expiry";
                }
                else {
                    main_Object.date_Type = "production";
                }

                if (main_Object.date_Type == "expiry") {
                    main_Object.date_CalcPeriod = "";
                    main_Object.date_CalcNo = "";
                }
                else {
                    main_Object.date_CalcPeriod = $("#datePeriod").val();
                    main_Object.date_CalcNo = parseInt($("#dateCalculation").val());

                }
            }
            else if (_main_idx == 3) {
                //alert(3);
                main_Object.weight_Start = $("#weightStart").val();
                main_Object.weight_End = $("#weightEnd").val();
                main_Object.weight_Format = $("#weightFormat").val();
            }
        }


            $("#itemEnd").val(main_Object.item_End);
            $("#lotStart").val(main_Object.lot_Start);
            $("#lotEnd").val(main_Object.lot_End);
            $("#dateStart").val(main_Object.date_Start);
            $("#dateEnd").val(main_Object.date_End);
            $("#weightStart").val(main_Object.weight_Start);
            $("#weightEnd").val(main_Object.weight_End);

            $("#dateFormat").val(main_Object.date_Format);
            $("#weightFormat").val(main_Object.weight_Format);

            //alert(main_Object.date_Type);

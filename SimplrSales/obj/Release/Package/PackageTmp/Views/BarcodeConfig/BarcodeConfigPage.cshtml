        <script>

            var table = document.getElementById('use-row');
            var tableEntry = table.getElementsByTagName('td');
            var isToggling = false;

            function enableToggle(e) {
                console.log('enableToggle')
                isToggling = true;

                if (e.target !== table) {
                    toggle(e);
                }
            }

            function disableToggle() {
                console.log('disableToggle')
                isToggling = false;
                indicate_On_Controls();
            }

            function toggle(e) {
                var tempvalue = 0;

                if (isToggling === false) {
                    return;
                }

                console.log('toggle:', e.target);
                tempvalue = parseInt(e.target.id.toString().split('_')[2]);
                // 1-18  tempvalue = 5
                if (tempvalue >= availableStart && tempvalue <= availableEnd) {
                    //alert(availableStart); alert(availableEnd);
                    e.target.classList.toggle('active');
                }
            }

            function startBarcode() {
                table.onmousedown = enableToggle;

                for (var i = 0, il = tableEntry.length; i < il; i++) {
                    tableEntry[i].onmouseenter = toggle; //changes color
                }

                table.onmouseup = disableToggle;
            }

            startBarcode();

            function indicate_On_Controls() {
                // FROM BARCODE TO CONTROLS =====================
                //undefined active
                //   debugger;
                var startNo = -1;
                var endNo = 0;
                var clsName = '';

                for (i = 1; i < 51; i++) {
                    ctrlid = "cell_used_" + i;
                    clsName = $("#" + ctrlid).attr('class');

                    if (clsName == "active") {
                        if (startNo == -1)
                            startNo = i - 1;
                        else
                            endNo = i - 1;
                    }

                }
                if (endNo == 0) endNo = 0;// startNo;

                if (_main_idx == 0) {
                    $("#itemStart").val(startNo);
                    $("#itemEnd").val(endNo);
                }
                else if (_main_idx == 1) {
                    $("#lotStart").val(startNo);
                    $("#lotEnd").val(endNo);
                }
                else if (_main_idx == 2) {
                    $("#dateStart").val(startNo);
                    $("#dateEnd").val(endNo);
                }
                else if (_main_idx == 3) {
                    $("#weightStart").val(startNo);
                    $("#weightEnd").val(endNo);

                    temp = (endNo - startNo) + 1;
                    $("#weightFormat").val(getZeros(temp));
                }

            }

            function indicate_Barcode(fillzeroes) {
                // FROM CONTROLS TO BARCODE =====================
                //debugger;
                var startNo = 0;
                var endNo = 0;
                var startNo_inner = 0;
                var endNo_inner = 0;
                var ctrlid;
                var temp = 0;
                var i = 0;
                var loop = 0;
                var loop1 = 0;

                if (_main_idx == 0) {
                    startNo = document.getElementById("itemStart").value;
                    endNo = document.getElementById("itemEnd").value;
                }
                else if (_main_idx == 1) {
                    startNo = document.getElementById("lotStart").value;
                    endNo = document.getElementById("lotEnd").value;
                }
                else if (_main_idx == 2) {
                    startNo = document.getElementById("dateStart").value;
                    endNo = document.getElementById("dateEnd").value;
                }
                else if (_main_idx == 3) {
                    startNo = document.getElementById("weightStart").value;
                    endNo = document.getElementById("weightEnd").value;

                    if (fillzeroes != undefined) {
                        // GIVE FORMAT HERE...
                        temp = (endNo - startNo) + 1;
                        document.getElementById("weightFormat").value = getZeros(temp);
                    }
                }



                if (startNo == "") {
                    startNo = 0;
                }
                else {
                    startNo = parseInt(startNo);
                }
                if (endNo == "") {
                    endNo = 0;
                }
                else {
                    endNo = parseInt(endNo);
                }

                // CLEAR THE BARCODES =======================
                for (i = 1; i < 51; i++) {
                    ctrlid = "cell_used_" + i;
                    $("#" + ctrlid).removeClass("active active_allocated");
                }

                //debugger;
                for (loop = 0; loop < 4; loop++) {
                    startNo_inner = 0; endNo_inner = 0;
                    if (loop == 0 && _main_idx != 0) {
                        startNo_inner = document.getElementById("itemStart").value;
                        endNo_inner = document.getElementById("itemEnd").value;
                    }
                    else if (loop == 1 && _main_idx != 1) {
                        startNo_inner = document.getElementById("lotStart").value;
                        endNo_inner = document.getElementById("lotEnd").value;
                    }
                    else if (loop == 2 && _main_idx != 2) {
                        startNo_inner = document.getElementById("dateStart").value;
                        endNo_inner = document.getElementById("dateEnd").value;
                    }
                    else if (loop == 3 && _main_idx != 3) {
                        startNo_inner = document.getElementById("weightStart").value;
                        endNo_inner = document.getElementById("weightEnd").value;
                    }


                    if (startNo_inner == "") {
                        startNo_inner = 0;
                    }
                    else {
                        startNo_inner = parseInt(startNo_inner);
                    }
                    if (endNo_inner == "") {
                        endNo_inner = 0;
                    }
                    else {
                        endNo_inner = parseInt(endNo_inner);
                    }

                    if (startNo_inner > 0 && endNo_inner > 0) {
                        for (loop1 = startNo_inner; loop1 <= endNo_inner; loop1++) {
                            ctrlid = "cell_used_" + (loop1 + 1);
                            $("#" + ctrlid).addClass("active_allocated");
                        }
                    }


                }

                if (fillzeroes != undefined) {
                    // fillzeroes defined
                    if (startNo > endNo) {
                        //alert("Start Number should not be greater than End Number.");

                        var obj = {};
                        obj.title = "Information";
                        obj.message = "Start Number should not be greater than End Number!";
                        showAlertMessage(obj);
                        return;
                    }

                    //    (1>=10 && 1<=10)  && (3>=15 && 3<=15)   10-15   3-5
                    if (!((startNo >= availableStart && startNo <= availableEnd) && (endNo >= availableStart && endNo <= availableEnd))) {
                        //alert("Start Number and End Number should be available in Barcode.");

                        var obj = {};
                        obj.title = "Information";
                        obj.message = "Start Number and End Number should be available in Barcode!";
                        showAlertMessage(obj);
                        return false;
                    }
                }


                if (startNo == 0 || endNo == 0) {
                    return false;
                }

                if (startNo == -1 && endNo == 0) {
                    return false;
                }

                for (i = startNo; i <= endNo; i++) {
                    ctrlid = "cell_used_" + (i + 1);
                    $("#" + ctrlid).addClass("active");
                }

                return false;
            }

            function getZeros(count) {
                var zeros = '';
                for (let i = 1; i <= count ; i++) {
                    zeros += '0';
                }
                return zeros;
            }


            function isNumber(evt) {
                evt = (evt) ? evt : window.event;
                var charCode = (evt.which) ? evt.which : evt.keyCode;
                if (charCode > 31 && (charCode < 48 || charCode > 57)) {
                    return false;
                }
                return true;
            }

        </script>


    </div>
